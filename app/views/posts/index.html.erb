<% @posts.each do |p| %>
  <h2><%= p.title %><%= p.tag_list %> </h2>
  <p>
    <% p.tag_list.each do |tag| %>
      <%= link_to tag, tagged_path(tag: tag) %>
    <% end %>
  </p>
<% end %>

